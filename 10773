#include <stdio.h>
#include <stdlib.h>

#define max 100000

int top=-1;

int stack[max];

int pop(){
    if(top==-1){
        
    }
    else{
        return stack[top--];
    }
}

void push (int a){
    stack[++top]=a;
}

int main(){
    int tryNum,sum=0;
     scanf("%d",&tryNum);
    
    for(int i=0;i<tryNum;i++){
        int temp;
        scanf("%d",&temp);
        if(temp==0){
            top--;
        }
        else{
            push(temp);
        }
    }
    
    for(int i=top;i>-1;i--){
        sum+=pop();
    }
    printf("%d",sum);
}
